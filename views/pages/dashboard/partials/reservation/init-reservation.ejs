<div class="ordered-customers-item"  id="<%= reservation.id %>">
    <div class="ordered-customers-item-top">
        <h3>
            <%= reservation.customer.firstName %>
        </h3>
        <p>
            <%= reservation.customer.phoneNumber %>
        </p>
    </div>
    <div class="ordered-customers-item-bottom"><button>Chưa có</button>
        <p>
            <%= reservation.time.getHours() %>:<%= reservation.time.getMinutes() %>
        </p>
        <p>
            <%= reservation.numberOfPeople %> người
        </p>
    </div>
</div>